page {
  background-color: #fda73d;
}
.table {
  width: 750rpx;
}
.u-ur {
  width: 750rpx;
}
.search {
  margin-top: 10rpx;
}
.search-name {
  display: inline-block;
  width: 500rpx;
  margin-left: 15rpx;
}
.search-identify {
  background-color: #f1f1f1;
  display: inline-block;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  width: 200rpx;
  margin-left: 20rpx;
}
.search-identify text {
  margin-top: 21rpx;
}
.table {
  width: 730rpx;
  overflow: hidden;
  margin: auto;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding: 20rpx;
}
image {
  width: 70rpx;
  -webkit-box-flex: 0;
  -webkit-flex: 0 0 120rpx;
          flex: 0 0 120rpx;
  height: 100rpx;
  margin-right: 20rpx;
  -webkit-border-radius: 12rpx;
          border-radius: 12rpx;
}
.title,
.title1 {
  text-align: left;
  font-size: 28rpx;
  color: #606266;
  margin-top: 20rpx;
}
.title1 {
  margin-left: 60rpx;
}
.members {
  background-color: #ccc;
}
.changeForm {
  width: 640rpx;
  margin: auto;
}
.form-title {
  width: 100%;
  margin: auto;
}
.operate-button {
  width: 500rpx;
  margin: auto;
}
.location {
  margin-left: 50rpx;
